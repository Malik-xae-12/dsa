class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] result = new int[nums.length];
        int i=0;
        for(int ele:nums){
            result[i]=nums[i];
            i++;
        }
        int pp=1;
        for(int j=0;j<nums.length;j++){
            result[j]=pp;
            pp=nums[j]*pp;
        }
        int sp=1;
        for(int k=nums.length-1;k>=0;k--){
            result[k]=result[k]*sp;
            sp=nums[k]*sp;
        
        }
        return result;
    }
}
